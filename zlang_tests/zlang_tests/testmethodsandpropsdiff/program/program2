SimpleFirstClass
	public
		name = "Bill"
		getName (out)nameParam
			nameParam = name
			return true

SimpleSecondClass
	public
		name (out)nameParam
			nameParam = "123"
			return true

EntryPoint : SimpleSecondClass
	public
		run
			a = SimpleFirstClass
			b = a.getName(c)
			print(c)
