EntryPoint
	public
		run
			sum = 0
			for i = 0; i < 10; i++
				sum += i
				if i == 5
					break
			println(sum)
			break

			sum = 0
			for i = 0; i < 10; i++
				if i < 5
					continue
				sum += i
			println(sum)
			continue

			sum = 0
			i = 0
			while (i < 10)
				sum += i
				if i == 5
					break
				i++
			println(sum)

			if (i > 10)
				continue

			sum = 0
			i = -1
			while (i < 9)
				i++
				if i < 5
					continue
				sum += i
			println(sum)

			map = Hashtable
			map[1] = 2
			map[3] = 4
			map[5] = 6
			map[7] = 8

			foreach pair in map
				if pair.value == 2
					continue
				if (pair.value == 8)
					break
				print(pair.key)
				print(":")
				println(pair.value)				
			break
			return true			
			