
EntryPoint
	public
		run
			a = 1
			a += 2
			print("1 += 2 = ")
			println(a)
			b = 1
			b -= 3
			print("1 -= 3 = ")
			println(b)
			b *= 4
			print("-2 *= 4 = ")
			println(b)
			b /= 3
			print("-8 /= 3 = ")
			println(b)
			a = b * 3
			print("-2 * 3 = ")
			println(a)
			a = b + 3
			print("-2 + 3 = ")
			println(a)
			b = 4
			a = b / 3
			print("4 / 3 = ")
			println(a)
			a = b - 3
			print("4 - 3 = ")
			println(a)
			a = 4
			a /= 3.0
			print("4 /= 3.0 = ")
			println(a)
			a = 4
			a *= 3.3
			print("4 *= 3.3 = ")
			println(a)
			a = 5 ^ 6
			print("5 ^ 6 = ")
			println(a)
			a = 4
			a = a ^ 6
			print("4(a) ^ 6 = ")
			println(a)
			a = 3
			a ^= 6
			print("3 ^= 6 = ")
			println(a)
			a = 5 | 6
			print("5 | 6 = ")
			println(a)
			a = 4
			a = a | 6
			print("4(a) | 6 = ")
			println(a)
			a = 3
			a |= 6
			print("3 |= 6 = ")
			println(a)
			a = 5 & 6
			print("5 & 6 = ")
			println(a)
			a = 4
			a = a & 6
			print("4(a) & 6 = ")
			println(a)
			a = 3
			a &= 6
			print("3 &= 6 = ")
			println(a)
			return true